{
  "name": "flash",
  "productName": "FlashDesk",
  "description": "Remote Macro",
  "keywords": [],
  "main": "./main.js",
  "version": "3.0.3",
  "author": "AmakoYoko",
  "scripts": {
    "start": "electron .",
    "pack": "npx electron-builder --dir",
    "dist": "npx electron-builder"
  },
  "build": {
    "appId": "fdl-01",
    "extraResources": [
      {
        "from": "./mp3.exe",
        "to": "mp3.exe",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "dependencies": {
    "audic": "^1.0.2",
    "electron-root-path": "^1.0.16",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "jsdom": "^16.2.2",
    "net": "^1.0.2",
    "node-mp3-player": "^1.0.3",
    "play-sound": "^1.1.3",
    "robotjs": "^0.6.0",
    "sendkeys": "^1.1.1",
    "sendkeys-js": "0.0.4",
    "sleep": "^6.2.0",
    "sound-play": "^1.0.5",
    "system-sleep": "^1.3.6"
  },
  "devDependencies": {
    "electron": "^8.3.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.11.0",
    "node-key-sender": "^1.0.11"
  },
  "license": "ISC",
  "postinstall": "electron-builder install-app-deps"
